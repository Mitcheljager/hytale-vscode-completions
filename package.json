{
  "name": "hytale-completions",
  "displayName": "Hytale Completions",
  "description": "Auto complete and hover info for Hytale mods and related bits",
  "version": "1.1.1",
  "publisher": "Mitsiee",
  "repository": {
    "type": "git",
    "url": "https://github.com/Mitcheljager/hytale-vscode-completions"
  },
  "engines": {
    "vscode": "^1.108.1"
  },
  "categories": [
    "Programming Languages",
    "Language Packs"
  ],
  "icon": "icon.png",
  "activationEvents": [
    "onStartupFinished",
    "onLanguage:plaintext",
    "onLanguage:json",
    "onLanguage:jsonc",
    "onLanguage:yaml",
    "onLanguage:java"
  ],
  "main": "./out/extension.js",
  "files": [
    "out/**/*",
    "data/**/*",
    "schema/**/*",
    "images/**/*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "hytale-completions.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "dependencies": {
    "jsonc-parser": "^3.3.1"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.108.1",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.52.0"
  }
}
